<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
 "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" 
 "struts-2.3.dtd">

<struts>




	<package name="salsman" extends="struts-default" namespace="/salesman">
		<action name="login" class="com.business.action.salesman.SalesmanAction"
			method="login">

			<result type="stream">
				<param name="contentType">text/html</param>

				<param name="inputName">bis</param>
			</result>


		</action>
		<action name="addUser" class="com.business.action.salesman.SalesmanAction"
			method="addUser">

			<result type="stream">
				<param name="contentType">text/html</param>
				<param name="inputName">bis</param>
			</result>
		</action>
		<action name="delUser" class="com.business.action.salesman.SalesmanAction"
			method="delUser">


			<result type="stream">
				<param name="contentType">text/html</param>
				<param name="inputName">bis</param>
			</result>
		</action>

		<action name="queryUserByName" class="com.business.action.salesman.SalesmanAction"
			method="selectByName">

			<result type="stream">
				<param name="contentType">text/html</param>
				<param name="inputName">bis</param>
			</result>
		</action>
		<action name="queryUserById" class="com.business.action.salesman.SalesmanAction"
			method="selectById">

			<result type="stream">
				<param name="contentType">text/html</param>
				<param name="inputName">bis</param>
			</result>
		</action>
		<action name="getUserAll" class="com.business.action.salesman.SalesmanAction"
			method="selectAll">

			<result type="stream">
				<param name="contentType">text/html</param>
				<param name="code">utf-8</param>

				<param name="inputName">bis</param>
			</result>
		</action>




	</package>


</struts>